{
  "name": "bud",
  "version": "0.0.1",
  "description": "Your simple budget management app.",
  "main": "./src/server/index.js",
  "repository": "git@github.com:Magellol/bud.git",
  "author": "Thomas Lefebvre <lefebvre515@gmail.com>",
  "scripts": {
    "dev": "NODE_ENV='development' nodemon ./src/server",
    "test:server": "NODE_ENV=${NODE_ENV:-'test'} mocha ./test",
    "test:client": "NODE_ENV=${NODE_ENV:-'test'} mocha ./src/client/**/*-test.js --require ./src/scripts/null-compiler --require ./src/scripts/jsdom-node --require babel-core/register",
    "test": "yarn run test:server && yarn run test:client",
    "watch": "webpack --config webpack.config.development.js --watch",
    "build": "webpack --config webpack.config.production.js",
    "start": "node ./src/server",
    "lint": "eslint ./",
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo",
    "db:seed": "sequelize db:seed:all"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-http": "^3.0.0",
    "dotenv": "^2.0.0",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "fetch-mock": "^5.8.1",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.4.1",
    "sinon": "^1.17.7"
  },
  "dependencies": {
    "autoprefixer": "^6.6.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "co": "^4.6.0",
    "compression": "^1.6.2",
    "config": "^1.24.0",
    "css-loader": "^0.26.1",
    "date-fns": "^1.25.0",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "mysql": "^2.12.0",
    "postcss-loader": "^1.2.1",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "request": "^2.79.0",
    "sequelize": "^3.28.0",
    "sequelize-cli": "^2.5.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "whatwg-fetch": "^2.0.1"
  },
  "engines": {
    "node": "7.2.1"
  }
}
